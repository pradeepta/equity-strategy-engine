{
  "name": "trading-strategy-dsl",
  "version": "1.0.0",
  "description": "TypeScript DSL for algorithmic trading strategies with compiler and runtime engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stocks-trading-mcp": "./dist/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "mcp": "node dist/mcp-server.js",
    "mcp:dev": "ts-node mcp-server.ts",
    "demo": "ts-node demo.ts",
    "demo:build": "tsc && node dist/demo.js",
    "backtest": "ts-node backtest.ts",
    "backtest:build": "tsc && node dist/backtest.js",
    "live": "ts-node live.ts",
    "live:build": "tsc && node dist/live.js",
    "live:multi": "ts-node live-multi.ts",
    "live:multi:build": "tsc && node dist/live-multi.js",
    "strategy:add": "ts-node cli/add-strategy.ts",
    "strategy:list": "ts-node cli/list-strategies.ts",
    "strategy:close": "ts-node cli/close-strategy.ts",
    "strategy:rollback": "ts-node cli/rollback-strategy.ts",
    "strategy:export": "ts-node cli/export-strategy.ts",
    "test": "jest",
    "test:tws": "ts-node test-tws.ts",
    "movers": "ts-node fetch-movers.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "dsl",
    "compiler",
    "runtime",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "dotenv": "^16.6.1",
    "ib": "^0.2.9",
    "jsep": "^1.3.8",
    "pg": "^8.17.1",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "jest": "^29.7.0",
    "prisma": "^7.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
