{
  "name": "trading-strategy-dsl",
  "version": "1.0.0",
  "description": "TypeScript DSL for algorithmic trading strategies with compiler and runtime engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stocks-trading-mcp": "./dist/mcp-server.js"
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "mcp": "node dist/mcp-server.js",
    "mcp:dev": "ts-node mcp-server.ts",
    "mcp:dev:debug": "node --inspect=9231 -r ts-node/register mcp-server.ts",
    "mcp:http": "node dist/mcp-server-http.js",
    "mcp:http:dev": "ts-node mcp-server-http.ts",
    "portfolio:api": "node dist/portfolio-api-server.js",
    "portfolio:api:dev": "ts-node portfolio-api-server.ts",
    "portfolio:api:dev:debug": "node --inspect=9230 -r ts-node/register portfolio-api-server.ts",
    "live:multi": "ts-node live-multi.ts",
    "live:multi:debug": "node --inspect=9229 -r ts-node/register live-multi.ts",
    "live:multi:build": "npm run build && node dist/live-multi.js",
    "dev:all": "concurrently --names \"LIVE,API,WEB,AI,MCP,TWS\" --prefix-colors \"cyan,green,yellow,magenta,blue,red\" \"npm run live:multi\" \"npm run portfolio:api:dev\" \"cd web-client && npm run dev -- -p 8001\" \"cd ai-gateway-live && npm run dev\" \"npm run mcp:dev\" \"cd tws-bridge-server && source venv/bin/activate && python server.py\"",
    "dev:all:debug": "concurrently --names \"LIVE,API,WEB,AI,MCP,TWS\" --prefix-colors \"cyan,green,yellow,magenta,blue,red\" \"npm run live:multi:debug\" \"npm run portfolio:api:dev:debug\" \"cd web-client && npm run dev -- -p 8001\" \"cd ai-gateway-live && npm run dev:debug\" \"npm run mcp:dev:debug\" \"cd tws-bridge-server && source venv/bin/activate && DEBUG_ENABLED=true python server.py\"",
    "tws:bridge": "cd tws-bridge-server && source venv/bin/activate && python server.py",
    "tws:bridge:debug": "cd tws-bridge-server && source venv/bin/activate && DEBUG_ENABLED=true python server.py",
    "strategy:add": "ts-node cli/add-strategy.ts",
    "strategy:list": "ts-node cli/list-strategies.ts",
    "strategy:close": "ts-node cli/close-strategy.ts",
    "strategy:rollback": "ts-node cli/rollback-strategy.ts",
    "strategy:export": "ts-node cli/export-strategy.ts",
    "strategy:compile": "ts-node cli/test-compile.ts",
    "test": "jest",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:tws": "ts-node test-tws.ts",
    "test:realtime": "ts-node test-realtime-bars.ts",
    "movers": "ts-node fetch-movers.ts",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "postinstall": "prisma generate && bash tws-bridge-server/setup.sh",
    "setup:tws": "bash tws-bridge-server/setup.sh"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "dsl",
    "compiler",
    "runtime",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@stoqey/ib": "^1.5.3",
    "axios": "^1.13.3",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "ib": "^0.2.9",
    "jsep": "^1.3.8",
    "openai": "^6.16.0",
    "pg": "^8.17.1",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.19.0",
    "winston-transport": "^4.9.0",
    "ws": "^8.19.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "prisma": "^7.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.17"
  }
}
